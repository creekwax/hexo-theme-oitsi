<div class="card bio">
    <div class="card-header">
        <div><%= __('Impression')%></div>
    </div>
    <div class="card-body">
        <div class="image"><img class="actual-image" src="<%- config.bio.avatar %>"></div>
        <div class="intro"><%= config.bio.nickname %><p><%= config.bio.intro %></p></div>
        <div class="contacts">
            <% for(var i in config.bio.social) { %>
                <p><%= __(i) %>: 
                    <% if (i == "Mail") { %>
                        <a href="mailto:<%- config.bio.social[i] %>" target="_blank"><%= config.bio.social[i] %></a>
                    <% } else { %>
                        <a href="<%- config.bio.social[i] %>" target="_blank"><%= config.bio.social[i] %></a>
                    <% } %>
                </p>
            <% } %>
        </div>
    </div>
</div>